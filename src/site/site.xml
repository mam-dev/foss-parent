<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.1 http://maven.apache.org/xsd/decoration-1.0.1.xsd"
    xmlns="http://maven.apache.org/DECORATION/1.0.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <version position="right" />
    <publishDate format="yyyy-MM-dd HH:mm" position="right" />
    <poweredBy>
        <logo name="Maven" href="http://maven.apache.org/"
            img="http://maven.apache.org/images/logos/maven-feather.png" />
        <logo name="1&amp;1 at github" href="https://github.com/1and1/"
            img="https://assets.github.com/images/modules/header/logov7-hover.png" />
    </poweredBy>
    <bannerLeft>
        <name>1&amp;1</name>
        <src>https://secure.gravatar.com/avatar/4bc599826789a6d0265a406b7e64e012</src>
        <href>http://blog.1und1.de/</href>
        <height>45</height>
        <width>45</width>
    </bannerLeft>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>
    <custom>
        <fluidoSkin>
            <searchEnabled>true</searchEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <gitHub>
                <projectId>1and1/${project.artifactId}</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>gray</ribbonColor>
            </gitHub>
            <googlePlusOne />
        </fluidoSkin>
    </custom>
  <body>
    <breadcrumbs>
      <item name="1&amp;1" href="http://blog.1und1.de/" />
      <item name="1&amp;1 FOSS" href="https://github.com/1and1/" />
      <item name="${project.name}" href="${project.url}" />
    </breadcrumbs>
    <menu ref="parent" />
    <menu ref="modules" />
    <menu ref="reports" inherit="bottom" />
  </body>
</project>

